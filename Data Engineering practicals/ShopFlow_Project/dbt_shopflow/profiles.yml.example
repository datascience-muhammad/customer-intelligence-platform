# Example `profiles.yml` for local development using Postgres
# Copy this to ~/.dbt/profiles.yml and update values or create an environment-specific profile.

shopflow:
  target: dev
  outputs:
    dev:
      type: postgres
      host: localhost
      user: your_db_user
      password: your_db_password
      port: 5432
      dbname: your_database
      schema: analytics
      threads: 4
      keepalives_idle: 0
